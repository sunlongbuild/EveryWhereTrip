<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_personalInfo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_10">

        <ImageView
            android:id="@+id/iv_headPhoto"
            android:layout_width="@dimen/dp_70"
            android:layout_height="@dimen/dp_70"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_37"
            android:src="@drawable/f" />

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_93"
            android:layout_marginTop="@dimen/dp_42"
            android:text="林小姐"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_22" />

        <TextView
            android:id="@+id/tv_write"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_14"
            android:layout_marginLeft="@dimen/dp_95"
            android:layout_marginTop="@dimen/dp_72"
            android:text="@string/sizhibuquan"
            android:textColor="@color/c_999999"
            android:textSize="@dimen/sp_12" />

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_239"
            android:layout_marginTop="@dimen/dp_94"
            android:src="@drawable/icon_me_bianji" />

        <TextView
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_17"
            android:layout_marginLeft="@dimen/dp_259"
            android:layout_marginTop="@dimen/dp_91"
            android:text="编辑"
            android:textSize="@dimen/sp_14" />
    </RelativeLayout>

    <android.support.v7.widget.CardView
        android:layout_width="@dimen/dp_279"
        android:layout_height="@dimen/dp_122"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_145"

        app:cardCornerRadius="@dimen/dp_5">

        <RelativeLayout
            android:layout_width="@dimen/dp_279"
            android:layout_height="@dimen/dp_122"
            android:background="@drawable/gra_dl_money">

            <TextView
                android:layout_width="@dimen/dp_58"
                android:layout_height="@dimen/dp_17"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_8"
                android:text="@string/my_money"
                android:textColor="@color/c_FFFFFF"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="@dimen/dp_70"
                android:layout_height="@dimen/dp_58"
                android:layout_marginLeft="@dimen/dp_101"
                android:layout_marginTop="@dimen/dp_29"
                android:gravity="center"
                android:text="99"
                android:textColor="@color/c_FFFFFF"
                android:textSize="@dimen/sp_48" />

            <TextView
                android:layout_width="@dimen/dp_14"
                android:layout_height="@dimen/dp_17"
                android:layout_marginLeft="@dimen/dp_174"
                android:layout_marginTop="@dimen/dp_60"
                android:text="元"
                android:textColor="@color/c_FFFFFF"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="@dimen/dp_90"
                android:layout_height="@dimen/dp_17"
                android:layout_marginLeft="@dimen/dp_195"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_94"
                android:text="赢取奖金>>"
                android:textColor="@color/c_FFFFFF"
                android:textSize="@dimen/sp_14" />

        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cd_cardView"
        android:layout_width="@dimen/dp_279"
        android:layout_height="@dimen/dp_212"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_286"
        app:cardCornerRadius="@dimen/dp_5">

        <RelativeLayout
            android:layout_width="@dimen/dp_288"
            android:layout_height="@dimen/dp_212">

            <ImageView
                android:id="@+id/iv_coupon"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_11"
                android:layout_marginLeft="@dimen/dp_11"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/icon_me_kaquan" />

            <TextView
                android:id="@+id/tv_myCoupon"
                android:layout_width="@dimen/dp_58"
                android:layout_height="@dimen/dp_17"
                android:layout_marginTop="@dimen/dp_17"
                android:layout_toRightOf="@id/iv_coupon"
                android:text="@string/tv_myCoupon"
                android:textColor="@color/c_333333"
                android:textSize="@dimen/sp_14" />

            <ImageView
                android:layout_width="@dimen/dp_6"
                android:layout_height="@dimen/dp_11"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_17"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/right_dl" />

            <View
                android:id="@+id/divid"
                android:layout_width="@dimen/dp_268"
                android:layout_height="@dimen/dp_2"
                android:layout_below="@id/tv_myCoupon"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_18"
                android:background="@color/c_F0F0F0" />


            <ImageView
                android:id="@+id/iv_shopping"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_11"
                android:layout_below="@id/divid"
                android:layout_marginLeft="@dimen/dp_11"
                android:layout_marginTop="@dimen/dp_19"
                android:src="@drawable/icon_me_yigou" />

            <TextView
                android:id="@+id/tv_shoppingTrip"
                android:layout_width="@dimen/dp_58"
                android:layout_height="@dimen/dp_17"
                android:layout_below="@id/divid"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_toRightOf="@id/iv_shopping"
                android:text="@string/tv_shoppingTrip"
                android:textColor="@color/c_333333"
                android:textSize="@dimen/sp_14" />

            <ImageView
                android:layout_width="@dimen/dp_6"
                android:layout_height="@dimen/dp_11"
                android:layout_alignParentRight="true"
                android:layout_below="@id/divid"
                android:layout_marginRight="@dimen/dp_17"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/right_dl" />

            <View
                android:id="@+id/divid2"
                android:layout_width="@dimen/dp_268"
                android:layout_height="@dimen/dp_2"
                android:layout_below="@id/tv_shoppingTrip"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_18"
                android:background="@color/c_F0F0F0" />

            <ImageView
                android:id="@+id/iv_collect"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_11"
                android:layout_below="@id/divid2"
                android:layout_marginLeft="@dimen/dp_11"
                android:layout_marginTop="@dimen/dp_19"
                android:src="@drawable/icon_me_shoucang" />

            <TextView
                android:id="@+id/tv_myCollect"
                android:layout_width="@dimen/dp_58"
                android:layout_height="@dimen/dp_17"
                android:layout_below="@id/divid2"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_toRightOf="@id/iv_shopping"
                android:text="@string/tv_myCollect"
                android:textColor="@color/c_333333"
                android:textSize="@dimen/sp_14" />

            <ImageView
                android:layout_width="@dimen/dp_6"
                android:layout_height="@dimen/dp_11"
                android:layout_alignParentRight="true"
                android:layout_below="@id/divid2"
                android:layout_marginRight="@dimen/dp_17"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/right_dl" />

            <View
                android:id="@+id/divid3"
                android:layout_width="@dimen/dp_268"
                android:layout_height="@dimen/dp_2"
                android:layout_below="@id/tv_myCollect"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_18"
                android:background="@color/c_F0F0F0" />

            <ImageView
                android:id="@+id/iv_yaoqing"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_11"
                android:layout_below="@id/divid3"
                android:layout_marginLeft="@dimen/dp_11"
                android:layout_marginTop="@dimen/dp_19"
                android:src="@drawable/icon_me_yaoqing" />

            <TextView
                android:id="@+id/tv_following"
                android:layout_width="@dimen/dp_58"
                android:layout_height="@dimen/dp_17"
                android:layout_below="@id/divid3"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_toRightOf="@id/iv_yaoqing"
                android:text="@string/follow"
                android:textColor="@color/c_333333"
                android:textSize="@dimen/sp_14" />

            <ImageView
                android:layout_width="@dimen/dp_6"
                android:layout_height="@dimen/dp_11"
                android:layout_alignParentRight="true"
                android:layout_below="@id/divid3"
                android:layout_marginRight="@dimen/dp_17"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/right_dl" />

        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_550">

        <ImageView
            android:id="@+id/kefu"
            android:layout_width="@dimen/dp_14"
            android:layout_height="@dimen/dp_13"
            android:layout_below="@id/cd_cardView"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginTop="@dimen/dp_129"
            android:src="@drawable/icon_me_kefu" />

        <TextView
            android:id="@+id/tv_custom"
            android:layout_width="@dimen/dp_56"
            android:layout_height="@dimen/dp_16"
            android:layout_below="@id/cd_cardView"
            android:layout_marginTop="@dimen/dp_127"
            android:layout_toRightOf="@id/kefu"
            android:text="@string/custom"
            android:textSize="@dimen/dp_14" />

        <ImageView
            android:id="@+id/iv_sugesst"
            android:layout_width="@dimen/dp_14"
            android:layout_height="10dp"
            android:layout_below="@id/cd_cardView"
            android:layout_marginLeft="@dimen/dp_34"
            android:layout_marginTop="@dimen/dp_132"
            android:layout_toRightOf="@id/tv_custom"
            android:src="@drawable/icon_me_fankui" />

        <TextView
            android:id="@+id/tv_sugesst"
            android:layout_width="@dimen/dp_56"
            android:layout_height="@dimen/dp_16"
            android:layout_below="@id/cd_cardView"
            android:layout_marginTop="@dimen/dp_127"
            android:layout_toRightOf="@id/iv_sugesst"
            android:text="@string/tv_sugesst"
            android:textSize="@dimen/dp_14" />

        <ImageView
            android:id="@+id/iv_version"
            android:layout_width="@dimen/dp_14"
            android:layout_height="10dp"
            android:layout_below="@id/cd_cardView"
            android:layout_marginLeft="@dimen/dp_34"
            android:layout_marginTop="@dimen/dp_132"
            android:layout_toRightOf="@id/tv_sugesst"
            android:src="@drawable/icon_me_banben" />

        <TextView
            android:layout_width="@dimen/dp_56"
            android:layout_height="@dimen/dp_16"
            android:layout_below="@id/cd_cardView"
            android:layout_marginTop="@dimen/dp_127"
            android:layout_toRightOf="@id/iv_version"
            android:text="版本检测"
            android:textSize="@dimen/dp_14" />
    </LinearLayout>
</RelativeLayout>